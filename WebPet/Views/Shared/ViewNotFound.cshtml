@model Guid?

@{
    ViewBag.Title = "404 Error";
}

<div class="alert alert-danger mt-1 mb-1">
    <h4>404 Not Found Error :</h4>
    <hr />

    @if (@Model == null)
    {
        <h5>
             @ViewBag.Name cannot be found
        </h5>
    }
    else
    {   <h5>
            @ViewBag.Name with ID = @Model cannot be found
        </h5>
    }
</div>

<a asp-controller="Custommers" asp-action="index" class="btn btn-outline-success"
   style="width:auto">Click here to see the list of all Custommers</a>